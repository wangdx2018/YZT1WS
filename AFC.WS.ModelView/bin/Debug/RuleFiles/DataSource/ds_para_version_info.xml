<?xml version="1.0"?>
<DataSourceRule xmlnsxsi="http//www.w3.org/2001/XMLSchema-instance" xmlnsxsd="http//www.w3.org/2001/XMLSchema" DataSourceName="ds_para_version_info" DataSourceTypeName="AFC.WS.UI.DataSources.DefaultDataSource,AFC.WS.UI.FC" ComboBoxDataSource="DefaultDataSource">
  <PropertyValues>
    <PropertyValue Key="DBConnectionString" />
    <PropertyValue Value="select bpti.para_desc,t.* from para_version_info t left join basi_para_type_info bpti on bpti.para_type=t.para_type " Key="Sql" />
    <PropertyValue Key="GroupParams" />
    <PropertyValue Key="HavingParams" />
    <PropertyValue Value="t.para_type in('4043','4044','4045','4042','4314') and t.para_version='-1'" Key="WhereParams" />
    <PropertyValue Value="t.para_version" Key="OrderByParams" />
  </PropertyValues>
</DataSourceRule>
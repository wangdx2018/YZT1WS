<local:UserControlBase x:Class="AFC.WS.UI.Primission.RoleAdded"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   xmlns:local="clr-namespace:AFC.BOM2.UIController;assembly=AFC.BOM2.UIController"
   xmlns:commonControls="clr-namespace:AFC.WS.UI.Components;assembly=AFC.WS.UI.FC"
                       Background="{DynamicResource MainBackgroundBrush}">
    <DockPanel>
        <Border Background="{DynamicResource DiagramGradientBrush}" Style="{DynamicResource BorderStyle}" x:Name="DiagramBorder">
            <DockPanel>
                <Grid x:Name="rootLayout">
                    <GroupBox Header="角色信息" Grid.Column="0" Grid.Row="0">
                        <commonControls:InteractiveControl x:Name="ic" />
                    </GroupBox>

                </Grid>
            </DockPanel>
        </Border>
    </DockPanel>
</local:UserControlBase>

<local:UserControlBase x:Class="AFC.WS.UI.UIPage.RunManager.SysStartAndCheck"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   xmlns:local="clr-namespace:AFC.BOM2.UIController;assembly=AFC.BOM2.UIController"
                        Background="{DynamicResource MainBackgroundBrush}"  >
    <DockPanel>
        <Border Background="{DynamicResource DiagramGradientBrush}"  Style="{DynamicResource BorderStyle}" x:Name="DiagramBorder">
            <DockPanel>
                <Grid x:Name="rootLayout">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="10*"></RowDefinition>
                        <RowDefinition Height="80*"></RowDefinition>
                        <RowDefinition Height="10*"></RowDefinition>
                    </Grid.RowDefinitions>

                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="10*"></ColumnDefinition>
                        <ColumnDefinition Width="80*"></ColumnDefinition>
                        <ColumnDefinition Width="10*"></ColumnDefinition>
                    </Grid.ColumnDefinitions>

                    <GroupBox x:Name="groupSysCheck" Grid.Row="1" Grid.Column="1" Header="系统自检">
                        <Grid >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="25*"></RowDefinition>
                                <RowDefinition Height="10*"></RowDefinition>
                                <RowDefinition Height="10*"></RowDefinition>
                                <RowDefinition Height="10*"></RowDefinition>
                                <RowDefinition Height="10*"></RowDefinition>
                                <RowDefinition Height="10*"></RowDefinition>
                                <RowDefinition Height="25*"></RowDefinition>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="10*"></ColumnDefinition>
                                <ColumnDefinition Width="40*"></ColumnDefinition>
                                <ColumnDefinition Width="40*"></ColumnDefinition>
                                <ColumnDefinition Width="10*"></ColumnDefinition>
                            </Grid.ColumnDefinitions>
                            <Label x:Name="labSCConnectionContent" Grid.Row="1" Grid.Column="1"/>
                            <Label x:Name="labSCConnectionResult" Grid.Row="1" Grid.Column="2"/>
                            <Label x:Name="labDBConnectionContent" Grid.Row="2" Grid.Column="1"/>
                            <Label x:Name="labDBConnectionResult" Grid.Row="2" Grid.Column="2"/>
                            <Label x:Name="labTimeSynContent" Grid.Row="3" Grid.Column="1"/>
                            <Label x:Name="labTimeSynResult" Grid.Row="3" Grid.Column="2"/>
                            <Label x:Name="labFTPConnectionContent" Grid.Row="4" Grid.Column="1"/>
                            <Label x:Name="labFTPConnectionResult" Grid.Row="4" Grid.Column="2"/>
                            <Label x:Name="labSoftwareUpdate" Grid.Row="5" Grid.Column="1"/>
                            <Label x:Name="labSoftwareUpdateResult" Grid.Row="5" Grid.Column="2"/>
                            <Button x:Name="btn" Content="退出"  Grid.Row="6" Grid.Column="2" Width="77.5" Height="22.5"/>
                        </Grid>
                    </GroupBox>
                </Grid>
            </DockPanel>
        </Border>
    </DockPanel>
</local:UserControlBase>

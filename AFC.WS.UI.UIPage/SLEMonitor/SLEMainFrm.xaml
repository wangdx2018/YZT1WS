<local:UserControlBase x:Class="AFC.WS.UI.UIPage.SLEMonitor.SLEMainFrm"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   xmlns:local="clr-namespace:AFC.BOM2.UIController;assembly=AFC.BOM2.UIController"
                       xmlns:selMonitor="clr-namespace:AFC.WS.UI.UIPage.SLEMonitor">
    <Grid x:Name="rootLayout">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="15*"/>
            <ColumnDefinition Width="85*"/>
            <ColumnDefinition Width="25*"/>

        </Grid.ColumnDefinitions>

        <selMonitor:StationTreeViewControl Grid.Column="0" x:Name="tv"/>
        <selMonitor:SLEDeviceMonitorControl Grid.Column="1" x:Name="sleMonitor"/>
        <selMonitor:DevStatusDetail Grid.Column="2" x:Name="devStatusDetail"/>
    </Grid>
</local:UserControlBase>

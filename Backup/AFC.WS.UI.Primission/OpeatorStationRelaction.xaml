<local:UserControlBase x:Class="AFC.WS.UI.Primission.OpeatorStationRelaction"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:AFC.BOM2.UIController;assembly=AFC.BOM2.UIController"
    xmlns:common="clr-namespace:AFC.WS.UI.CommonControls;assembly=AFC.WS.UI.FC"
                         Background="{DynamicResource MainBackgroundBrush}">
    <DockPanel>
        <Border Background="{DynamicResource DiagramGradientBrush}" Style="{DynamicResource BorderStyle}" x:Name="DiagramBorder">
            <DockPanel>
                <Grid>
                    <common:RelactionContol x:Name="operatorStation" ></common:RelactionContol>
                </Grid>
            </DockPanel>
        </Border>
    </DockPanel>
</local:UserControlBase>

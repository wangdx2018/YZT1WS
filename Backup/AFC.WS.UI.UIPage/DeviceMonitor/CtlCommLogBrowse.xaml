<local:UserControlBase x:Class="AFC.WS.UI.UIPage.DeviceMonitor.CtlCommLogBrowse"
  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   xmlns:local="clr-namespace:AFC.BOM2.UIController;assembly=AFC.BOM2.UIController" Background="{DynamicResource MainBackgroundBrush}" Style="{DynamicResource ucStyle}"
   xmlns:commonControl="clr-namespace:AFC.WS.UI.Components;assembly=AFC.WS.UI.FC" >
    <DockPanel>
        <Border Background="{DynamicResource DiagramGradientBrush}" Style="{DynamicResource BorderStyle}" x:Name="DiagramBorder">

            <DockPanel>
                <Grid x:Name="rootLayout">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="3"></RowDefinition>
                        <RowDefinition Height="18*"></RowDefinition>
                        <RowDefinition Height="82*"></RowDefinition>
                    </Grid.RowDefinitions>
                    <GroupBox x:Name="ControlLogQueryInfo" Grid.Row="1" Header="控制命令日志查询">
                            <commonControl:InteractiveControl x:Name="icControl" />
                        </GroupBox>
                        <GroupBox x:Name="ControlLogInfo" Grid.Row="2" Header="控制命令日志信息">
                            <commonControl:DataListControl x:Name="dataList"/>
                        </GroupBox>
                </Grid>
            </DockPanel>


        </Border>
    </DockPanel>
    </local:UserControlBase>


<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <section name="ZipFileName" type="System.Configuration.DictionarySectionHandler" />
  </configSections>
  <ZipFileName>
    <add key="fileName1" value="SCWS.sfx.exe" />
    <add key="fileNamehistory" value="SCWS1.sfx.exe" />
    <add key="fileName2" value="WsFileVerInfo.xml" />
  </ZipFileName>
  <appSettings>
    <add key="LCWS" value="true" />
    <add key="SCWS" value="true" />
    <add key="TCWS" value="true" />
    <!--主程序文件名称-->
    <add key="WsApplicationName" value="SCWS" />
    <!--Ws当前版本存放路径-->
    <add key="ParamFTPDownLoadPath" value="\VersionFile\ParaCurrent\" />
    <add key="TempFTPDownLoadPath" value="\VersionFile\ParaTemp\" />
